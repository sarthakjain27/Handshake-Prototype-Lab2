{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":"off",
        "no-unused-vars":"off",
        "array-callback-return":"off",
        "class-methods-use-this":"off",
        "no-underscore-dangle":"off",
        "camelcase":"off",
        "no-shadow":"off",
        "no-restricted-syntax":"off",
        "eqeqeq":"off",
        "no-param-reassign":"off",
        "no-plusplus":"off",
        "max-len":"off",
        "consistent-return":"off"
    }
}